<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<template id="assets_frontend" inherit_id="website.assets_frontend">
		<xpath expr="//script[last()]" position="after">
			<script type="text/javascript" src="/widgets/static/src/js/button.js"></script>
			<script type="text/javascript" src="/widgets/static/src/js/chatter.js"></script>
		</xpath>
		 <xpath  expr = "// link [last ()]"  position = "after" >
            <link src="/widgets/static/src/sass/chatter.scss"></link>
        </xpath> 
	</template>
	<template id="assets_backend" inherit_id = "web.assets_backend">
         <xpath  expr = "// script [last ()]"  position = "after" >
            <script type="text/javascript" src="/widgets/static/src/js/field.js"></script>
        </xpath> 
    </template>
</odoo>